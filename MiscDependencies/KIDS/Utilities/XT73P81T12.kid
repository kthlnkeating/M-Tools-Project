 ;
"RTN","XTMLOG",5,0)
 ; Routine provides logging capability similar in various
"RTN","XTMLOG",6,0)
 ; respects to Log4J.
"RTN","XTMLOG",7,0)
 ;
"RTN","XTMLOG",8,0)
 D EN^XTMUNIT("XTMTSTL1")
"RTN","XTMLOG",9,0)
